{
    "rules": {
        ".read": "false",
        ".write": "false",
        "sessions": {
            ".read": "auth != null",
            ".indexOn": "end"
        },
        "users": {
            "$uid": {
                ".read": "root.child('users').child(auth.uid).child('moderator').exists() || $uid === auth.uid"
            }
        }
    }
}
